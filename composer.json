{
    "autoload": {
        "psr-4": {"Example\\": "src/",
                  "DocuSign\\OAuth2\\Client\\Provider\\": "src/"
                 }
    },
    "autoload-dev": {
        "psr-4": {
            "Example\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@code-sniffer"
        ],
        "post-update-cmd": [
            "@code-sniffer"
        ],
        "test": "./vendor/bin/phpunit --verbose tests"
    },
    "description": "DocuSign PHP launcher",
    "require": {
        "docusign/admin-client": "^1.3.0-rc",
        "docusign/click-client": "^1.3.0-rc",
        "docusign/esign-client": "^6.15.0-rc",
        "docusign/rooms-client": "^2.1.0",
        "docusign/monitor-client": "^1.1.0",
        "twig/twig": "^3.5.1",
        "league/oauth2-client": "^2.6.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "7.5.0",
        "firebase/php-jwt": "5.5.1",
        "mashape/unirest-php": "3.0.4",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "^9.5"
    }
}
